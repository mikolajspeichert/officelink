{
  "name": "officelink",
  "version": "0.1.0",
  "description": "Small conference app for Thinksmart.pl",
  "main": "main.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config server/config/webpack.prod.babel.js --color --define process.env.NODE_ENV=\"production\" --progress",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "NODE_ENV=production node server",
    "start": "NODE_ENV=development node server",
    "electrize": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Mikolaj Speichert",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.1.0",
    "compression": "^1.7.0",
    "electron": "~1.6.2",
    "express": "^4.15.4",
    "helmet": "^3.8.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "ip": "^1.1.5",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.11",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webrtc-adapter": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.5.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "circular-dependency-plugin": "^3.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-css-modules": "^2.7.3",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.2.1",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.0",
    "prettier": "^1.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-0",
      "react"
    ],
    "env": {
      "test": {
        "plugins": ["rewire", "transform-async-to-generator"]
      }
    },
    "ignore": [
      "/node_modules/",
      "/build/chunks/",
      "/build/public/",
      "/build/updates/",
      "/build/server.js"
    ]
  }
